<div class="section">
  <% if latest %>
    <h1 class="heading"><%= latest.currency %></h1>
    <div class="row text-block">
      <div class="col bold">
        <div>Date</div>
        <div>Price</div>
        <div>Bid</div>
        <div>Ask</div>
      </div>
      <div class="col right">
        <div><%= latest.price_at %></div>
        <div><%= latest.last %></div>
        <div><%= latest.bid %></div>
        <div><%= latest.ask %></div>
      </div>
    </div>
  <% else %>
    <div class="text-block">No data available. Click 'Capture' button.</div>
  <% end %>
</div>